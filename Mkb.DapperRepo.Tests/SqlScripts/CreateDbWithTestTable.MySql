create database PlaceHolderDbName;

create table PlaceHolderDbName.TableWithAutoIncrementPrimaryKey
(
    Id         int auto_increment,
    Name       varchar(150) null,
    SomeNumber int          null,
    constraint TableWithAutoIncrementPrimaryKey_pk
        primary key (Id)
);

create table PlaceHolderDbName.TableWithNoAutoGeneratedPrimaryKey
(
    Id         varchar(60)  not null,
    Name       varchar(150) null,
    SomeNumber int          null,
    constraint TableWithAutoIncrementPrimaryKey_pk
        primary key (Id)
);

